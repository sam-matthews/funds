\! echo "======================="
\! echo "Create Table: portfolio_price_history"

CREATE TABLE IF NOT EXISTS portfolio_price_history(
  p_date      DATE,
  p_portfolio CHAR(10),
  p_service   CHAR(15),
  p_fund      CHAR(10),
  p_sma       REAL,
  p_perc      REAL,
  p_score     INT,
  p_total     REAL
);

